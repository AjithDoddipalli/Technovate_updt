<body>
  {{>navbar}}

  {{!-- hero section starts --}}
  <div class="container-fluid mt-5">
    <div class="row">
      <div class="col-md-10 col-12 mx-auto">
        <div class="row">
          <div class="col-md-6 col-12 hero-text order-md-0 order-1 d-flex justify-content-center align-items-start flex-column">
            <h1>WE WILL HELP YOU TO</h1>
            <h1>GROW YOUR BUSINESS</h1>
            <p>We are a team of Professional <span class="bold-style">IT Consultants.</span></p>
            <button class="btn-style btn-primary"> <a href="https://en.wikipedia.org/wiki/List_of_IT_consulting_firms"> Read More</a></button>
          </div>
          {{!-- right side --}}
          <div class="col-md-6 col-12 hero-text order-md-1 order-0">
            <figure>
              <img src="\images\growth.svg" alt="hrosection_image">
            </figure>
          </div>
        </div>
      </div>
    </div>
  </div>
  {{!-- hero section ends --}}

  {{!-- about section starts --}}
  {{>about}}
  {{!-- about section ends --}}

  {{!-- service section starts --}}
  {{>service}}
  {{!-- service section ends --}}

  {{!-- map section starts --}}
  {{>maps}}
  {{!-- map section ends --}}

  {{!-- contact section starts --}}
  {{>contact}}
  {{!-- contact section ends --}}


  <script src="../jq/jquery.js" type="text/javascript"></script>
  <script src="../js/bootstrap.js" type="text/javascript"></script>
